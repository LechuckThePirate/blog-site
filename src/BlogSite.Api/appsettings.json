{
  "https_port": 443,
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "mysql": "Server={{DB_HOST}};Database={{DB_NAME}};Uid={{DB_USER}};Pwd={{DB_PASSWORD}};Port={{DB_PORT}}"
  },
  "Kestrel": {
    "Certificates": {
      "Default": {
        "Path": "/root/ssl/cert.pem",
        "KeyPath": "/root/ssl/key.pem"
      }
    }
  }
}
